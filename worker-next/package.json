{
  "name": "@flightcard/worker-next",
  "version": "0.0.0",
  "type": "module",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241022.0",
    "@types/node": "22.9.0",
    "puppeteer": "^23.7.1",
    "typescript": "^5.6.3",
    "wrangler": "^3.83.0"
  },
  "private": true,
  "scripts": {
    "cf-codegen": "wrangler types",
    "start": "wrangler dev --test-scheduled src/index.ts",
    "deploy": "wrangler deploy && npm run db:migrate --remote",
    "db:do": "wrangler d1 execute flightcard --command",
    "db:migrate:reset": "npm run db:do \"drop table migrations\"",
    "db:migrate": "wrangler d1 migrations apply flightcard",
    "nar:update": "npx tsx src/nar_update.ts"
  },
  "dependencies": {
    "@flightcard/common-types": "*"
  }
}
